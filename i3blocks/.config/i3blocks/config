command=/usr/lib/i3blocks/$BLOCK_NAME
separator_block_width=45
markup=none

[cpu_usage]
interval=10

[memory]
interval=10

[disk]
interval=10

[load_average]
interval=10

[iface]
interval=10

[bandwidth]
interval=10

[battery]
interval=10

[backlight]
command=$HOME/.config/i3blocks/$BLOCK_NAME 15
signal=2
interval=10

[keyboard_backlight]
command=$HOME/.config/i3blocks/$BLOCK_NAME
signal=3
interval=10

[volume]
signal=1
interval=10

[time]
command=$HOME/.config/i3blocks/$BLOCK_NAME
interval=1
separator_block_width=15
